<div class="col-xs-12 col-sm-12 col-md-7 col-lg-6 status_div">

<div class="col-xs-12 col-sm-8 col-md-7 col-lg-8 pull-left">

<form>

  <div class="form-group">
  	<div class="col-xs-5 col-sm-6 col-md-6 col-lg-6 text-right"><label for="" class="lbl2">Your subscription ends by :</label></div>
  	<div class="col-xs-7 col-sm-6 col-md-6 col-lg-6 pull-left">
          <div class="pull-left">
            <input type="text" class="subscription" id="" placeholder="" style="float:left;">
            <img src="../images/alert1.gif" style="float:left;"/>
		 </div>
    </div>
  	<div class="col-xs-7 col-sm-6 col-md-6 col-lg-6 pull-right text-left"><a href="/complaint_logger/home/subscription" class="pay_link"><b>Click to pay</b></a></div>
  
  
  
  <Div class="clearfix"></Div>
  </div>

  
</form>



</div>

<!--login details start-->
<div class="col-xs-12 col-sm-4 col-md-5 col-lg-4 text-right">
  <span id="dLabel" class="login" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
    <%= OrgMail%>
    <span class="caret"></span>
  </span>
	
  <ul class="dropdown-menu login-menu" aria-labelledby="dLabel">
    <li class="text-right"><button type="button" class="btn btn-primary" onClick="signOut();">Sign Out</button>
    <div class="clearfix"></div>
    </li>
  </ul>
</div>
<!--login details end-->



</div>




</div>



<div class="col-xs-12"><h2 class="title"><%= organization%> Dashboard</h2>

</div>

<hr class="title_brd" />


<div class="col-xs-12">

<table class="table table-bordered">
<tr class="tbl_head">
          <th>Si</th>
          <th>Date</th>
          <th>Complaint Header</th>
          <th>User</th>
          <th>Status</th>
          <th class="col-xs-1">View</th>
</tr>
<% var i=0%>
<% user.forEach(function(item){%>
<tr>
<% i++ %>
<td><%= i %></td>
<td><%= item.date%></td>
<td><%= item.header%></td>
<td><%= item.name%></td>
<% if(item.status ==0){ %>
<td>Submitted</td>
<% }else if(item.status ==1){%>
<td>Processing</td>
<% }else if(item.status ==2){%>
<td>Completed</td>
<%} else if(item.status ==3){%>
<td>Discarded</td>
<%}else {%>
<td>No status</td>
<%}%>

<td><input type="button" class="btn btn btn-sm btn1" value = "Details" id = "" onClick = "editDetails(<%= item.cId%>);"></td>
</tr>
<%});%>

</table>


</div>




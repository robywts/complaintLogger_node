	<div class="row">
        <div class="col-xs-12 "><h2 class="title">Complaint :</h2><h2 class="title complaint_id"><%= cData['complaint_id']%></h2></div>
        <hr class="title_brd" />
	</div>



	<div class="col-xs-12 col-sm-6 col-md-1 col-lg-1"><label for="" class="lbl1">Date</label></div>
    
	<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><input type="text" class="form-control" id="" placeholder="" value ="<%= cData['date']%>" name="date"></div>
    
	<div class="col-xs-12 col-sm-6 col-md-1 col-lg-1"><label for="" class="lbl1">User</label></div>
    
	<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><input type="text" class="form-control" id="" placeholder="" value = "<%= cData['username']%>" name="username"></div>
    
	<div class="col-xs-12 col-sm-6 col-md-1 col-lg-1"><label for="" class="lbl1">Mobile</label></div>
    
	<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><input type="text" class="form-control" id="" placeholder="" value = "<%= cData['mobile']%>" name="mobile"></div>




<div class="col-xs-12">
<label for="" class="lbl1">Complaint Head</label>
</div>

<div class="col-xs-12">
<textarea name="header" class="form-control" rows="1"><%= cData['header']%></textarea>
</div>


<div class="col-xs-12">
<label for="" class="lbl1">Complaint Description</label>
</div>

<div class="col-xs-12">
<textarea name="description" class="form-control" rows="3"><%= cData['description']%></textarea>
</div>


<div class="col-xs-12">
<label for="" class="lbl1">Change Status</label></div>

<div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">

<select name="status" class="form-control" id="">
   
<option value ="0" <%if(cData['status']==0){%>selected<%}%>>Submitted</option>
<option value ="1" <%if(cData['status']==1){%>selected<%}%>>Processing</option>
<option value ="2" <%if(cData['status']==2){%>selected<%}%>>Completed</option>
<option value ="3" <%if(cData['status']==3){%>selected<%}%>>Discarded</option>
  </select>
</div>


<div class="col-xs-12">
<label for="" class="lbl1">Remarks</label>
</div>

<div class="col-xs-12">
<textarea name="remark" class="form-control" rows="3"><%= cData['remark']%></textarea>
</div>
<input type="hidden" class="form-control" id="" placeholder="" value = "<%= cData['cId']%>" name="complaintId">

<div class="row row1">
<div class="col-xs-12">
<div class="col-xs-12 col-sm-4 col-md-4 col-lg-3">
<% imageUrl.forEach(function(item){%>
<img src="http://220.227.57.26/complaint_logger<%= item.photo_url%>" class="img-responsive photo" alt="Responsive image"/></div>
<% }); %>
</div>
</div>

